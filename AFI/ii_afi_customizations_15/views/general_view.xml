<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>
    <!-- Company View Customization: adding cost and sale price mark-up constant -->
    <record id="is_inherited_view_company_form" model="ir.ui.view">
      <field name="name">is.inherited.view.company.form</field>
      <field name="model">res.company</field>
      <field name="inherit_id" ref="base.view_company_form"/>
      <field name="arch" type="xml">
        <field name="name" position="after">
          <field name="company_header" widget="image"/>
          <field name="company_footer" widget="image"/>
        </field>
      </field>
    </record>
  </data>
</odoo>
